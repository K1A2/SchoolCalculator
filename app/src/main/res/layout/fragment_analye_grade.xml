<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:paddingBottom="10dp"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".fragment.AnalyeTypeFragment"
        android:orientation="vertical"
        android:paddingRight="10dp"
        android:paddingLeft="10dp">

        <androidx.cardview.widget.CardView
            app:cardElevation="4dp"
            app:contentPadding="10dp"
            app:cardUseCompatPadding="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="전체 성적 분석"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:fontFamily="@font/scdream3"
                    android:textColor="#000000"/>

                <com.github.mikephil.charting.charts.LineChart
                    android:padding="5dp"
                    android:layout_marginTop="5dp"
                    android:id="@+id/analye_chart_analyzeA"
                    android:layout_width="match_parent"
                    android:layout_height="150dp"
                    android:layout_marginBottom="10dp"></com.github.mikephil.charting.charts.LineChart>

            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            app:cardElevation="2dp"
            app:contentPadding="10dp"
            app:cardUseCompatPadding="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="학년별 분석"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:fontFamily="@font/scdream3"
                    android:textColor="#000000"/>

                <com.github.mikephil.charting.charts.LineChart
                    android:padding="5dp"
                    android:layout_marginTop="5dp"
                    android:id="@+id/analye_chart_analyze"
                    android:layout_width="match_parent"
                    android:layout_height="150dp"
                    android:layout_marginBottom="10dp"></com.github.mikephil.charting.charts.LineChart>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp"
                    android:orientation="horizontal"
                    android:layout_marginBottom="3dp">

                    <TextView
                        android:visibility="invisible"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="g"
                        android:textSize="15dp"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:fontFamily="@font/scdream3"
                        android:textColor="#000000"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="1학년"
                        android:textSize="15dp"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:fontFamily="@font/scdream3"
                        android:textColor="#000000"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="2학년"
                        android:textSize="15dp"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:fontFamily="@font/scdream3"
                        android:textColor="#000000"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="3학년"
                        android:textSize="15dp"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:fontFamily="@font/scdream3"
                        android:textColor="#000000"/>

                </LinearLayout>

                <View
                    android:layout_marginTop="3dp"
                    android:layout_marginBottom="3dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorDivider"/>

                <LinearLayout
                    android:layout_marginBottom="3dp"
                    android:layout_marginTop="3dp"
                    android:layout_width="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="1학기"
                        android:textSize="15dp"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:fontFamily="@font/scdream3"
                        android:textColor="#000000"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="1학년"
                        android:textSize="15dp"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:id="@+id/fragment_gradeAll_11"
                        android:fontFamily="@font/scdream3"
                        android:textColor="#000000"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="2학년"
                        android:textSize="15dp"
                        android:id="@+id/fragment_gradeAll_21"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:fontFamily="@font/scdream3"
                        android:textColor="#000000"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="3학년"
                        android:id="@+id/fragment_gradeAll_31"
                        android:textSize="15dp"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:fontFamily="@font/scdream3"
                        android:textColor="#000000"/>

                </LinearLayout>

                <View
                    android:layout_marginTop="3dp"
                    android:layout_marginBottom="3dp"
                    android:layout_width="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp"
                    android:layout_height="1dp"
                    android:background="@color/colorDivider"/>

                <LinearLayout
                    android:layout_marginBottom="3dp"
                    android:layout_marginTop="3dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="2학기"
                        android:textSize="15dp"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:fontFamily="@font/scdream3"
                        android:textColor="#000000"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="1학년"
                        android:id="@+id/fragment_gradeAll_12"
                        android:textSize="15dp"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:fontFamily="@font/scdream3"
                        android:textColor="#000000"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="2학년"
                        android:textSize="15dp"
                        android:id="@+id/fragment_gradeAll_22"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:fontFamily="@font/scdream3"
                        android:textColor="#000000"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="3학년"
                        android:textSize="15dp"
                        android:id="@+id/fragment_gradeAll_32"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:fontFamily="@font/scdream3"
                        android:textColor="#000000"/>

                </LinearLayout>

                <View
                    android:layout_marginTop="3dp"
                    android:layout_marginBottom="3dp"
                    android:layout_width="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp"
                    android:layout_height="1dp"
                    android:background="@color/colorDivider"/>

                <TextView
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="3학년"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:textSize="15dp"
                    android:id="@+id/fragment_opinion_1"
                    android:gravity="center_vertical"
                    android:layout_weight="1"
                    android:fontFamily="@font/scdream3"
                    android:textColor="#000000"/>

                <TextView
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="3학년"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:textSize="15dp"
                    android:id="@+id/fragment_opinion_2"
                    android:gravity="center_vertical"
                    android:layout_weight="1"
                    android:fontFamily="@font/scdream3"
                    android:textColor="#000000"/>

                <TextView
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="3학년"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:textSize="15dp"
                    android:id="@+id/fragment_opinion_3"
                    android:gravity="center_vertical"
                    android:layout_weight="1"
                    android:fontFamily="@font/scdream3"
                    android:textColor="#000000"/>

            </LinearLayout>

        </androidx.cardview.widget.CardView>

    </LinearLayout>

</androidx.core.widget.NestedScrollView>